<Window x:Class="GUI.PopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="LogOut" Height="200" Width="300"
        ResizeMode="CanMinimize">
    <Grid>
        <Button x:Name="NoButton" Content="No" HorizontalAlignment="Left" Margin="171,106,0,0" VerticalAlignment="Top" Width="75" Click="NoButton_Click"/>
        <Button x:Name="YesButton" Content="Yes" HorizontalAlignment="Left" Margin="42,106,0,0" VerticalAlignment="Top" Width="75" Click="YesButton_Click"/>
        <Label x:Name="label" Content="Are you sure that you want to log out?" HorizontalAlignment="Left" Margin="38,53,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>